{
  "arrowParens": "always",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": false,
  "bracketSameLine": true,
  "importOrder": [
    "lodash/(.*)$",
    "@mui/(.*)$",
    "^api/(.*)$",
    "^app/(.*)$",
    "^models/(.*)$",
    "^constants/(.*)$",
    "^recoil/(.*)$",
    "^reduxToolkit/(.*)$",
    "^utils/(.*)$",
    "^hooks/(.*)$",
    "^locales/(.*)$",
    "^assets/(.*)$",
    "^icons/(.*)$",
    "^components/(.*)$",
    "^features/(.*)$",
    "^features/(.*)/api$",
    "^features/(.*)/api/(.*)$",
    "^features/(.*)/models$",
    "^features/(.*)/models/(.*)$",
    "^features/(.*)/constants$",
    "^features/(.*)/constants/(.*)$",
    "^features/(.*)/recoil$",
    "^features/(.*)/recoil/(.*)$",
    "^features/(.*)/hooks$",
    "^features/(.*)/hooks/(.*)$",
    "^features/(.*)/assets$",
    "^features/(.*)/assets/(.*)$",
    "^features/(.*)/components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
